{
 
  "compilerOptions": {
    "jsx": "react",
    "allowJs": true,
    "target": "ES6",
    "module": "ESNext",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "baseUrl": ".",
    "outDir": "dist",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "strict": false,
     "types": [
      "jest",
      "node",
      "jest-prosemirror"
    ],
    "paths": {
      "@assets/*": ["./assets/*"],
      "@src/*": ["./src/*"]
    }
  },
  "include": ["src/**/*", "tools/**/*"],
  "exclude": [
    "node_modules",
  ]
  
}