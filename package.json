{
  "name": "@modusoperandi/licit",
  "version": "1.2.0",
  "subversion": "1",
  "description": "Rich text editor built with React and ProseMirror",
  "main": "dist/index.js",
  "style": "dist/styles.css",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MO-Movia/licit.git"
  },
  "scripts": {
    "test": "jest --coverage",
    "build:bom": "cyclonedx-bom -o dist/bom.xml",
    "build:clean": "rm -rf dist/ && rm -f modusoperandi-licit-*.*.*.tgz",
    "build:css": "mkdirp ./dist/styles && cp src/styles/* dist/styles",
    "build:fonts": "mkdirp ./dist/fonts && cp -r fonts/* dist/fonts",
    "build:ts": "tsc -p tsconfig.json",
    "build:babel": "babel src --out-dir dist --extensions .ts",
    "build:dist": "npm run build:clean && npm run build:ts && npm run webpack && npm run build:babel && npm run build:css && npm run build:fonts && npm run build:bom",
    "webpack": "webpack",
    "serve": "webpack serve",
    "lint:css": "stylelint \"**/*.css\" --fix",
    "lint:ts": "eslint src --ext .ts,.tsx --fix",
    "start": "node scripts/webserver.js",
    "prepare": "npm run build:dist",
    "prebuild:style-service-docker": "node utils/build_customstyle_server.js",
    "build:style-service-docker": "docker build . -f style-service.Dockerfile -t style-service:latest"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.4",
    "@babel/plugin-proposal-do-expressions": "^7.10.4",
    "@babel/plugin-proposal-export-default-from": "^7.10.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.10.4",
    "@babel/plugin-proposal-function-sent": "^7.10.4",
    "@babel/plugin-proposal-json-strings": "^7.18.6",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.18.9",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-numeric-separator": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.9",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.18.9",
    "@babel/plugin-proposal-throw-expressions": "^7.18.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-parameters": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/plugin-transform-typescript": "^7.15.0",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.15.0",
    "@cyclonedx/bom": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "babel-jest": "^27.4.6",
    "babel-loader": "8.2.5",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "6.0.3",
    "css-loader": "3.6.0",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-react": "^7.24.0",
    "fake-indexeddb": "^3.1.7",
    "file-loader": "^6.1.0",
    "html-loader": "1.1.0",
    "html-webpack-inline-source-plugin": "1.0.0-beta.2",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^7.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.6",
    "jest-prosemirror": "^1.0.3",
    "jest-react-hooks-shallow": "^1.5.1",
    "lint-staged": "^11.1.1",
    "mkdirp": "^1.0.4",
    "prettier": "^2.1.2",
    "stylelint": "^13.7.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-prettier": "^1.2.0",
    "terser-webpack-plugin": "^3.1.0",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.2.0",
    "typescript": "^4.5.4",
    "webpack": "^4.44.2",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.9.2",
    "write-file-webpack-plugin": "4.5.1"
  },
  "dependencies": {
    "@modusoperandi/licit-doc-attrs-step": "^0.1.1",
    "@modusoperandi/licit-ui-commands": "^0.1.7",
    "@svgr/webpack": "^4.0.4",
    "@tiptap/extension-collaboration": "^2.1.0",
    "@tiptap/extension-collaboration-cursor": "^2.1.0",
    "@tiptap/extension-highlight": "*",
    "@tiptap/extension-link": "^2.1.0",
    "@tiptap/extension-list-item": "*",
    "@tiptap/extension-subscript": "^2.1.0",
    "@tiptap/extension-superscript": "^2.1.0",
    "@tiptap/extension-table": "^2.1.0",
    "@tiptap/extension-table-cell": "^2.1.0",
    "@tiptap/extension-table-header": "^2.1.0",
    "@tiptap/extension-table-row": "^2.1.0",
    "@tiptap/extension-task-item": "*",
    "@tiptap/extension-task-list": "*",
    "@tiptap/extension-text-align": "^2.1.0",
    "@tiptap/extension-underline": "^2.1.0",
    "@tiptap/react": "^2.0.0",
    "@tiptap/starter-kit": "^2.1.0",
    "@types/prosemirror-state": "^1.2.8",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "browserkeymap": "2.0.2",
    "classnames": "2.2.6",
    "color": "^3.1.2",
    "katex": "0.11.1",
    "node-mathquill": "0.10.2",
    "nullthrows": "1.1.1",
    "prosemirror-dev-tools": "^3.1.0",
    "prosemirror-inputrules": "^1.1.3",
    "prosemirror-tables": "^1.2.5",
    "prosemirror-utils": "^1.0.0-0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "resize-observer-polyfill": "1.5.1",
    "smooth-scroll-into-view-if-needed": "1.1.28",
    "style-loader": "^1.2.1",
    "url-loader": "^4.1.1",
    "uuid": "9.0.0",
    "y-indexeddb": "^9.0.6",
    "y-webrtc": "^10.2.3",
    "yjs": "^13.5.24"
  },
  "importSort": {
    ".js": {
      "parser": "babylon",
      "style": "module-grouping"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.css": [
      "stylelint --fix"
    ],
    "!(*test|*.setup).ts": [
      "eslint --fix"
    ],
    "*.json": [
      "prettier --write"
    ]
  }
}
