{
  "name": "@modusoperandi/licit-tiptap",
  "version": "1.0.5",
  "subversion": "1",
  "description": "Rich text editor built with React and ProseMirror",
  "type": "module",
  "main": "index.js",
  "style": "styles.css",
  "types": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MO-Movia/breaking-changes-tiptap.git"
  },
  "scripts": {
    "test": "jest --coverage",
    "build:bom": "cyclonedx-bom -o dist/bom.xml",
    "build:clean": "rm -rf dist/ && rm -f modusoperandi-licit-tiptap-*.*.*.tgz",
    "build:css": "mkdirp ./dist/styles && cp src/styles/* dist/styles",
    "build:fonts": "mkdirp ./dist/fonts && cp -r fonts/* dist/fonts",
    "build:assets": "mkdirp ./dist/assets && cp -r assets/* dist/assets",
    "build:ts": "tsc -p tsconfig.json",
    "build:dist": "npm run build:ts && npm run build:css && npm run build:fonts && npm run build:assets && npm run build:bom && copyfiles package.json dist && cp src/*.d.ts dist/ ",
    "webpack": "webpack",
    "start": "cross-env NODE_ENV=development webpack serve --config tools/webpack/webpack.config.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config tools/webpack/webpack.config.prod.js",
    "serve": "cross-env NODE_ENV=production webpack --config tools/webpack/webpack.config.prod.js && cross-env serve dist/",
    "lint:ts": "eslint src --ext .ts --fix",
    "lint:css": "stylelint \"**/*.css\" --fix",
    "lint": "npm run lint:css & npm run lint:ts",
    "prebuild:style-service-docker": "node utils/build_customstyle_server.js",
    "build:style-service-docker": "docker build . -f style-service.Dockerfile -t style-service:latest"
  },
  "devDependencies": {
    "@cyclonedx/bom": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/jest": "^29.5.14",
    "@types/node": "^18.11.5",
    "@types/webpack-env": "^1.18.0",
    "copy-webpack-plugin": "^12.0.2",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-prettier": "^5.5.4",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "html-webpack-plugin": "^5.5.0",
    "eslint-plugin-react": "^7.37.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.5",
    "jest-json": "^2.0.0",
    "jest-junit": "^16.0.0",
    "jest-prosemirror": "^3.0.1",
    "jest-sonar-reporter": "^2.0.0",
    "less": "^4.1.3",
    "less-loader": "11.1.0",
    "lint-staged": "^16.1.4",
    "mini-css-extract-plugin": "^2.6.1",
    "mkdirp": "^1.0.4",
    "prettier": "^3.6.2",
    "react-refresh": "^0.14.0",
    "sass": "^1.55.0",
    "sass-loader": "^13.1.0",
    "serve": "^14.0.1",
    "style-loader": "^3.3.1",
    "stylelint": "^16.23.0",
    "stylelint-config-standard": "^39.0.0",
    "stylelint-prettier": "^5.0.3",
    "ts-jest": "^29.4.1",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "@modusoperandi/licit-doc-attrs-step": "file:modusoperandi-licit-doc-attrs-step-1.0.3.tgz",
