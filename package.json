{
  "name": "@modusoperandi/licit",
  "version": "0.1.5",
  "subversion": "1",
  "description": "Rich text editor built with React and ProseMirror",
  "main": "dist/index.js",
  "style": "dist/styles.css",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MO-Movia/licit.git"
  },
  "scripts": {
    "test": "jest",
    "build:bom": "cyclonedx-bom -o dist/bom.xml",
    "build:clean": "rm -rf dist/ && rm -f modusoperandi-licit-*.*.*.tgz",
    "build:css": "cp src/ui/*.css dist/ui && cp src/ui/mathquill-editor/*.css dist/ui/mathquill-editor && cp src/client/*.css dist/client && cp src/*.css dist",
    "build:fonts": "mkdirp ./dist/fonts && cp -r fonts/* dist/fonts",
    "build:babel": "babel src --out-dir dist",
    "build:flow": "flow-copy-source -v -i '**/__tests__/**' src dist",
    "build:dist": "npm run build:clean && npm run build:babel && npm run build:flow && npm run build:css && npm run build:fonts && npm run build:bom",
    "build:licit": "node utils/build_bin.js",
    "flow": "flow --show-all-errors",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:js": "eslint \"src/**/*.js\" \"licit/**/*.js\"",
    "start": "node scripts/webserver.js",
    "prepare": "npm run build:dist",
    "prebuild:style-service-docker": "node utils/build_customstyle_server.js",
    "build:style-service-docker": "docker build . -f style-service.Dockerfile -t style-service:latest"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.15.8",
    "@babel/plugin-proposal-do-expressions": "^7.14.5",
    "@babel/plugin-proposal-export-default-from": "^7.14.5",
    "@babel/plugin-proposal-export-namespace-from": "^7.14.5",
    "@babel/plugin-proposal-function-sent": "^7.14.5",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.15.6",
    "@babel/plugin-proposal-pipeline-operator": "^7.15.0",
    "@babel/plugin-proposal-throw-expressions": "^7.14.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-flow-strip-types": "^7.14.5",
    "@babel/plugin-transform-parameters": "^7.15.4",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-flow": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@cyclonedx/bom": "^3.1.1",
    "babel-eslint": "10.1.0",
    "babel-jest": "^27.2.4",
    "babel-loader": "8.2.2",
    "babel-plugin-flow-react-proptypes": "26.0.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "9.0.1",
    "css-loader": "6.3.0",
    "enzyme": "^3.11.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.3",
    "eslint": "7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.5.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "7.26.1",
    "exports-loader": "^3.0.0",
    "file-loader": "^6.2.0",
    "flow-bin": "0.161.0",
    "flow-copy-source": "2.0.9",
    "flow-webpack-plugin": "1.2.0",
    "html-loader": "2.1.2",
    "html-webpack-inline-source-plugin": "1.0.0-beta.2",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.2.4",
    "lint-staged": "^11.2.0",
    "mkdirp": "^1.0.4",
    "prettier": "^2.4.1",
    "react-test-renderer": "^17.0.2",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-prettier": "^1.2.0",
    "terser-webpack-plugin": "^5.2.4",
    "webpack": "^5.57.1",
    "webpack-dev-server": "^4.3.1",
    "write-file-webpack-plugin": "4.5.1"
  },
  "dependencies": {
    "@modusoperandi/licit-doc-attrs-step": "^0.1.1",
    "@modusoperandi/licit-ui-commands": "file:modusoperandi-licit-ui-commands-0.0.1.tgz",
    "body-parser": "^1.19.0",
    "browserkeymap": "2.0.2",
    "classnames": "2.3.1",
    "color": "4.0.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "flatted": "^3.2.2",
    "flow-typed": "^3.3.1",
    "formidable": "^1.2.2",
    "invariant": "2.2.4",
    "jquery": "^3.6.0",
    "katex": "0.13.18",
    "mv": "^2.1.1",
    "node-mathquill": "0.10.2",
    "nullthrows": "1.1.1",
    "prosemirror-collab": "1.2.2",
    "prosemirror-commands": "1.1.11",
    "prosemirror-dev-tools": "3.1.0",
    "prosemirror-dropcursor": "1.3.5",
    "prosemirror-gapcursor": "1.2.0",
    "prosemirror-history": "1.2.0",
    "prosemirror-inputrules": "1.1.3",
    "prosemirror-keymap": "^1.1.4",
    "prosemirror-model": "^1.14.3",
    "prosemirror-state": "^1.3.4",
    "prosemirror-tables": "^1.1.1",
    "prosemirror-transform": "^1.3.3",
    "prosemirror-utils": "file:packages/prosemirror-utils-1.0.0-0.tgz",
    "prosemirror-view": "^1.20.2",
    "query-string": "7.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "resize-observer-polyfill": "1.5.1",
    "smooth-scroll-into-view-if-needed": "1.1.32",
    "stable": "0.1.8",
    "style-loader": "^3.3.0",
    "uuid": "8.3.2",
    "webfontloader": "1.6.28"
  },
  "importSort": {
    ".js": {
      "parser": "babylon",
      "style": "module-grouping"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.css": [
      "stylelint --fix"
    ],
    "!(*test|*.setup).js": [
      "eslint --fix"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "jest": {
    "rootDir": "src",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "../jest.setup.js"
    ]
  }
}
