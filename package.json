{
  "name": "@modusoperandi/licit",
  "version": "0.1.9",
  "subversion": "1",
  "description": "Rich text editor built with React and ProseMirror",
  "main": "dist/index.js",
  "style": "dist/styles.css",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MO-Movia/licit.git"
  },
  "scripts": {
    "test": "jest --coverage",
    "build:bom": "cyclonedx-bom -o dist/bom.xml",
    "build:clean": "rm -rf dist/ && rm -f modusoperandi-licit-*.*.*.tgz",
    "build:css": "cp src/components/Licit/licit.scss dist/components/Licit/licit.scss && cp src/components/Licit/extensions/toolbar/menu.css dist/components/Licit/extensions/toolbar/menu.css",
    "build:fonts": "mkdirp ./dist/fonts && cp -r fonts/* dist/fonts",
    "build:ts": "tsc -p tsconfig.json",
    "build:babel": "babel src --out-dir dist --extensions .ts",
    "build:dist": "npm run build:clean && npm run build:ts && npm run webpack && npm run build:babel && npm run build:css && npm run build:fonts && npm run build:bom",
    "webpack": "webpack",
    "lint:css": "stylelint \"**/*.css\" --fix",
    "lint:ts": "eslint \"src/**/*.ts[x]\" \"licit/**/*.ts[x]\" --fix",
    "start": "node scripts/webserver.js",
    "prepare": "npm run build:dist",
    "prebuild:style-service-docker": "node utils/build_customstyle_server.js",
    "build:style-service-docker": "docker build . -f style-service.Dockerfile -t style-service:latest"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/eslint-parser": "^7.15.0",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.4",
    "@babel/plugin-proposal-do-expressions": "^7.10.4",
    "@babel/plugin-proposal-export-default-from": "^7.10.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.10.4",
    "@babel/plugin-proposal-function-sent": "^7.10.4",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/plugin-proposal-pipeline-operator": "^7.10.4",
    "@babel/plugin-proposal-throw-expressions": "^7.10.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-parameters": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/plugin-transform-typescript": "^7.15.0",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.15.0",
    "@cyclonedx/bom": "^2.0.1",
    "@types/jest": "^27.0.3",
    "@types/node": "^15.12.2",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.6",
    "babel-eslint": "10.1.0",
    "babel-jest": "^27.4.6",
    "babel-loader": "8.1.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "6.0.3",
    "css-loader": "3.6.0",
    "enzyme": "^3.11.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.24.0",
    "exports-loader": "^1.1.0",
    "file-loader": "^6.1.0",
    "html-loader": "1.1.0",
    "html-webpack-inline-source-plugin": "1.0.0-beta.2",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^7.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.6",
    "jest-prosemirror": "^1.0.3",
    "jest-sonar-reporter": "^2.0.0",
    "lint-staged": "^11.1.1",
    "mkdirp": "^1.0.4",
    "prettier": "^2.1.2",
    "react-test-renderer": "^17.0.1",
    "stylelint": "^13.7.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-prettier": "^1.2.0",
    "terser-webpack-plugin": "^3.1.0",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.2.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^3.11.0",
    "write-file-webpack-plugin": "4.5.1"
  },
  "dependencies": {
    "@tiptap/react": "^2.0.0-beta.98",
    "@tiptap/starter-kit": "^2.0.0-beta.157",
    "body-parser": "^1.19.0",
    "browserkeymap": "2.0.2",
    "classnames": "2.2.6",
    "color": "3.1.2",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "flatted": "^3.1.0",
    "flow-typed": "^3.3.1",
    "formidable": "^1.2.2",
    "invariant": "2.2.4",
    "jquery": "^3.5.1",
    "katex": "0.11.1",
    "mv": "^2.1.1",
    "node-mathquill": "0.10.2",
    "nullthrows": "1.1.1",
    "prosemirror-dev-tools": "3.1.0",
    "query-string": "6.13.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "resize-observer-polyfill": "1.5.1",
    "smooth-scroll-into-view-if-needed": "1.1.28",
    "stable": "0.1.8",
    "style-loader": "^1.2.1",
    "uuid": "8.2.0",
    "webfontloader": "1.6.28"
  },
  "importSort": {
    ".js": {
      "parser": "babylon",
      "style": "module-grouping"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.css": [
      "stylelint --fix"
    ],
    "!(*test|*.setup).ts": [
      "eslint --fix"
    ],
    "*.json": [
      "prettier --write"
    ]
  }
}
