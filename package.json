{
  "name": "@modusoperandi/licit",
  "version": "1.2.0",
  "subversion": "1",
  "description": "Rich text editor built with React and ProseMirror",
  "type": "module",
  "main": "index.js",
  "style": "styles.css",
  "types": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MO-Movia/licit.git"
  },
  "scripts": {
    "test": "jest --coverage",
    "build:bom": "cyclonedx-bom -o dist/bom.xml",
    "build:clean": "rm -rf dist/ && rm -f modusoperandi-licit-*.*.*.tgz",
    "build:css": "mkdirp ./dist/styles && cp src/styles/* dist/styles",
    "build:fonts": "mkdirp ./dist/fonts && cp -r fonts/* dist/fonts",
    "build:ts": "tsc -p tsconfig.json",
    "build:babel": "babel src --out-dir dist --extensions .ts",
    "build:dist": "npm run build:clean && npm run build:ts && npm run webpack && npm run build:babel && npm run build:css && npm run build:fonts && npm run build:bom",
    "webpack": "webpack",
    "lint:css": "stylelint \"**/*.css\" --fix",
    "lint:ts": "eslint src --ext .ts,.tsx --fix",
    "start": "cross-env NODE_ENV=development webpack serve --config tools/webpack/webpack.config.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config tools/webpack/webpack.config.prod.js",
    "serve": "cross-env NODE_ENV=production webpack --config tools/webpack/webpack.config.prod.js && cross-env serve dist/",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src/",
    "prepare": "npm run build:dist",
    "prebuild:style-service-docker": "node utils/build_customstyle_server.js",
    "build:style-service-docker": "docker build . -f style-service.Dockerfile -t style-service:latest"
  },
  "devDependencies": {
    "@cfaester/enzyme-adapter-react-18": "^0.8.0",
    "@cyclonedx/bom": "^2.0.1",
    "@testing-library/dom": "^10.0.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.2",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.2",
    "@types/node": "^18.11.5",
    "@types/react": "^18.0.23",
    "@types/react-dom": "^18.0.7",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "enzyme": "^3.11.0",
    "eslint": "^8.26.0",
    "eslint-import-resolver-alias": "^1.1.2",
